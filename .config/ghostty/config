keybind = ctrl+a>h=goto_split:left
keybind = ctrl+a>j=goto_split:bottom
keybind = ctrl+a>k=goto_split:top
keybind = ctrl+a>l=goto_split:right
keybind = ctrl+enter=text:\r
keybind = super+shift+up=scroll_page_lines:-1
keybind = super+shift+down=scroll_page_lines:1

window-height = 2000
window-width = 2000

macos-titlebar-style = tabs
