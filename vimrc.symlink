syntax on
filetype plugin indent on
set encoding=utf-8
set title
set hls
set is
set showcmd
set ruler
set wildmenu
set cursorline
set autoindent
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set number

set background=dark
let g:solarized_termcolors=256
colorscheme solarized

inoremap jj <esc>
inoremap jk <esc>
map <C-n> :noh<CR>
map <C-m> :set relativenumber!<CR>
